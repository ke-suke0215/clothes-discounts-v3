{
  "name": "clothes-discounts-v3",
  "private": true,
  "description": "All-in-one remix starter template for Cloudflare Pages",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "cleanup": "rimraf .cache ./build",
    "deploy": "wrangler deploy",
    "dev": "remix vite:dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "prepare": "husky",
    "start": "wrangler dev",
    "test": "playwright test --ui",
    "typecheck": "wrangler types && tsc",
    "typegen": "wrangler types"
  },
  "lint-staged": {
    "*.{js,mjs,ts,tsx,css,md,yml}": "prettier --write"
  },
  "dependencies": {
    "@markdoc/markdoc": "^0.4.0",
    "@prisma/adapter-d1": "^6.0.1",
    "@prisma/client": "^6.0.1",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@remix-run/cloudflare": "^2.12.1",
    "@remix-run/cloudflare-pages": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "isbot": "^4.1.0",
    "lucide-react": "^0.468.0",
    "prisma": "^6.0.1",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@octokit/types": "^14.1.0",
    "@playwright/test": "^1.47.2",
    "@remix-run/dev": "^2.12.1",
    "@remix-run/eslint-config": "^2.12.1",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "msw": "^2.4.9",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "rimraf": "^6.0.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^3.4.16",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.2",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.22.0"
  },
  "engines": {
    "node": ">=20"
  }
}
